{
  "compilerOptions": {
    "outDir": "../../dist/routes",
    "target": "es2023",
    "lib": ["ES2023"],
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["./**/*.d.ts"]
}